# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'JenkinsiOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  platform :ios, '10.0'
  use_frameworks!
  pod 'SAMKeychain'
  pod 'Firebase/Core'
  pod 'Firebase/Performance'
  pod 'Firebase/RemoteConfig'
  pod 'Crashlytics'

  # Pods for JenkinsiOS

  target 'JenkinsiOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'JenkinsiOSTodayExtension' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  platform :ios, '10.0'
  use_frameworks!
  pod 'SAMKeychain'
  pod 'Firebase/Core'
  pod 'Firebase/Performance'
  pod 'Firebase/RemoteConfig'
  pod 'Crashlytics'

  # Pods for JenkinsiOSTodayExtension

end
